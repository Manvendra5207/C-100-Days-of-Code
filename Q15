#include <stdio.h>

int main() {
    char ch;
    
    printf("Enter a Character ");
    scanf("%c",&ch);
    
    if (ch>='a' && ch<='z')
    {
        printf("Character is Lowercase");
    }
    
    else if (ch>='A' && ch<='Z')
    {
        printf("Character is Uppercase");
    }
    
    else
    {
        printf("Character is special Character");
    }
    
    return 0;
}
