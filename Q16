#include <stdio.h>

int main() {
int a, b, c;
printf("Enter Three No: ");
scanf("%d %d %d",&a, &b, &c);
    
if (a>=b && a>=c)
    {
        printf("A is Greatest No");
    }
    
else if (b>=a && b>=c)
    {
        printf("B is Greatest No");
    }

else 
    {
        printf("C is Greatest No");
    }
    
    return 0;
}
