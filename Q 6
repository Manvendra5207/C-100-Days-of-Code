#include <stdio.h>

int main() {
    int a,b, temp;
    
     printf("Enter number a: ");
     scanf("%d",&a);
     
     printf("Enter number b: ");
     scanf("%d", &b);
     
     temp=a;
     a=b;
     b=temp;
     
    printf("Value of a: %d\n",a );
     printf("Value of b: %d",b );

    return 0;
}
