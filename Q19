#include <stdio.h>

int main() {
int a, b, c;
printf("Enter side of Triangles: ");
scanf("%d %d %d",&a, &b, &c);
    
if (a==b && b==c)
    {
        printf("Triangle is eqilateral");
    }
    
else if (a==b || a==c || b==c)
    {
        printf("Triange is Isosceles");
    }

else 
    {
        printf("Triangle is scalene");
    }
    
    return 0;
}
